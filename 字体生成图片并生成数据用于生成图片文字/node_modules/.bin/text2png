#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/lenovo/Desktop/FontToImge/node_modules/.store/text2png@2.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/lenovo/Desktop/FontToImge/node_modules/.store/text2png@2.3.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/text2png@2.3.0/node_modules/text2png/bin/text2png.js" "$@"
else
  exec node  "$basedir/../.store/text2png@2.3.0/node_modules/text2png/bin/text2png.js" "$@"
fi
