#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/github/GameTools/字体生成图片并生成数据用于生成图片文字/node_modules/.store/text2png@2.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/github/GameTools/字体生成图片并生成数据用于生成图片文字/node_modules/.store/text2png@2.3.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/text2png@2.3.0/node_modules/text2png/bin/text2png.js" "$@"
else
  exec node  "$basedir/../.store/text2png@2.3.0/node_modules/text2png/bin/text2png.js" "$@"
fi
